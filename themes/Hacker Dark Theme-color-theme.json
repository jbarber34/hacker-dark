      "settings": {
        "foreground": "#F5F520"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#FF6600"
      }
    },
    {
      "name": "CSS Class and Support",
